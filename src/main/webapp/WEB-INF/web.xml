<?xml version="1.0" encoding="UTF-8"?>
<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <welcome-file-list>
        <welcome-file>xiaopiao_verify.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>GetQuery</servlet-name>
        <servlet-class>Servlet.GetQueryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetQuery</servlet-name>
        <url-pattern>/Servlet/GetQuery</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ClearSession</servlet-name>
        <servlet-class>Servlet.ClearServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ClearSession</servlet-name>
        <url-pattern>/Servlet/ClearSession</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Voice</servlet-name>
        <servlet-class>Servlet.VoiceServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Voice</servlet-name>
        <url-pattern>/Servlet/Voice</url-pattern>
    </servlet-mapping>


</web-app>
